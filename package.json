{
  "name": "shareable-lists-api",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "npm run migrate:deploy && node dist/main.js",
    "start:dev": "npm run migrate:deploy && npm run build && npm run watch",
    "test-ci": "npm test",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "db:generate-client": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@apollo/server": "4.2.2",
    "@apollo/server-plugin-landing-page-graphql-playground": "4.0.0",
    "@apollo/server-plugin-response-cache": "4.1.0",
    "@apollo/subgraph": "2.2.1",
    "@aws-sdk/client-eventbridge": "^3.276.0",
    "@pocket-tools/apollo-utils": "3.2.0",
    "@prisma/client": "^4.9.0",
    "@sentry/node": "7.38.0",
    "@sentry/tracing": "7.38.0",
    "aws-sdk": "2.1092.0",
    "aws-xray-sdk-core": "3.4.1",
    "aws-xray-sdk-express": "3.4.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "graphql-upload": "15.0.2",
    "sinon": "^15.0.1",
    "slugify": "1.6.5",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@pocket-tools/eslint-config": "2.0.0",
    "@types/chai": "4.3.4",
    "@types/jest": "29.4.0",
    "@types/node": "^18.11.19",
    "chai": "4.3.7",
    "dotenv": "16.0.3",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "nock": "13.3.0",
    "nodemon": "2.0.20",
    "prisma": "^4.9.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
